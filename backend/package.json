{
  "name": "backend",
  "version": "0.4.0",
  "description": "SGCV2 Backend API - Sistema de Gesti√≥n y Control para XTEL Comunicaciones",
  "main": "dist/server.js",
  "types": "module",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "nodemon --watch src --ext ts,json --exec 'ts-node -r tsconfig-paths/register src/server.ts'",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint eslint.config.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "prisma:reset": "prisma migrate reset"
  },
  "keywords": [
    "sgcv2",
    "api",
    "express",
    "typescript"
  ],
  "author": "XTEL Comunicaciones",
  "license": "UNLICENSED",
  "type": "commonjs",
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@sgcv2/shared": "workspace:*",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "inversify": "^7.10.4",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.10.9",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "jest": "^30.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "nodemon": "^3.1.11",
    "prisma": "^7.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
